{
  "name": "aktivitetsplan",
  "version": "2.0.0",
  "description": "Aktivitetsplan",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build:dev-ekstern": "npm run build:tekster && vite build --mode=dev-ekstern && npm run manifest-fix",
    "build:dev-intern": "npm run build:tekster && vite build --mode=dev-intern && npm run manifest-fix",
    "build:prod-ekstern": "npm run build:tekster && vite build --mode=prod-ekstern && npm run manifest-fix",
    "build:prod-intern": "npm run build:tekster && vite build --mode=prod-intern && npm run manifest-fix",
    "build": "npm run build:tekster && vite build",
    "build:gh-pages": "VITE_USE_HASH_ROUTER=true vite build --mode=dev-ekstern --base=/aktivitetsplan/",
    "build:tekster": "node ./scripts/build-tekster.js",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "format-all": "prettier --write 'src/'",
    "lint-staged": "lint-staged",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "manifest-fix": "node ./scripts/fixManifest.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.11",
    "@navikt/ds-css": "^2.3.1",
    "@navikt/ds-icons": "^1.5.4",
    "@navikt/ds-react": "^2.2.0",
    "@navikt/ds-tailwind": "^2.3.0",
    "@navikt/navspa": "^5.0.1",
    "@nutgaard/use-formstate": "^3.0.1",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "history": "^4.10.1",
    "intl": "^1.2.5",
    "jest": "^29.4.3",
    "moment": "^2.29.4",
    "moment-duration-format": "^2.3.2",
    "moment-timezone": "^0.5.39",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-collapse": "^5.1.1",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.43.3",
    "react-intl": "^6.2.1",
    "react-modal": "^3.16.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^5.3.4",
    "rebound": "^0.1.0",
    "redux": "^4.2.0",
    "redux-freeze": "^0.1.7",
    "redux-thunk": "^2.4.2",
    "reselect": "^4.1.7",
    "rollup-plugin-visualizer": "^5.9.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^4.8.4",
    "vite": "^4.1.4",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-svgr": "^2.4.0",
    "vitest": "^0.29.1",
    "whatwg-fetch": "^3.6.2",
    "zod": "^3.20.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,less,css,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 10",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.5",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^29.2.1",
    "@types/moment-duration-format": "^2.2.3",
    "@types/node": "^18.11.9",
    "@types/react": "^17.0.45",
    "@types/react-collapse": "^5.0.1",
    "@types/react-dom": "^17.0.17",
    "@types/react-modal": "^3.13.1",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-c8": "^0.29.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.7.0",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "eslint": "^8.35.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.1",
    "import-sort-style-module": "^6.0.0",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.0.3",
    "msw": "^1.1.0",
    "postcss": "^8.4.21",
    "prettier": "^2.7.1",
    "prettier-plugin-import-sort": "0.0.7",
    "tailwindcss": "^3.2.7"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/navikt/aktivitetsplan.git"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
