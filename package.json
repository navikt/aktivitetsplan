  "scripts": {
    "test": "mocha",
    "test:watch": "mocha --watch",
    "test:coverage": "nyc --reporter=html npm test",
    "start": "node example/server.js",
    "test:e2e": "node test/integration/nightwatch.js -c test/integration/default.conf.js --skiptags browserstack",
    "test:browserstack": "node test/integration/browserstack.runner.js -c test/integration/browserstack.conf.js -e win_chrome,ios_safari,android_chrome,win_ie,win_edge --tag browserstack",
    "eslint": "eslint src --ext .js --ext .jsx",
    "eslint-staged": "eslint",
    "lesshint": "lesshint --reporter ./lesshint-reporter src/",
    "lesshint-staged": "lesshint --reporter ./lesshint-reporter",
    "lint": "npm run prettier && npm run eslint && npm run lesshint",
    "prettier": "prettier --write --single-quote --trailing-comma es5 --tab-width 4 src/**/*.js src/**/*.less",
    "dev:mock": "npm-run-all build:tekster --parallel dev:less dev:js:mock",
    "dev:mock:utside": "npm-run-all build:tekster --parallel dev:less \"dev:js:mock -- --env.utside\"",
    "dev:js:mock": "webpack-dev-server --env.dev --env.mock",
    "dev": "npm-run-all build:tekster --parallel dev:less dev:js",
    "dev:js": "webpack-dev-server --env.dev --port 8888",
    "dev:less": "nodemon -e less  --exec npm run build:less:dev",
    "build": "npm run build:tekster && npm run build:less && npm run build:js:sbs && npm run build:js:fss",
    "build:js": "webpack --optimize-minimize",
    "build:less:dev": "node ./node_modules/less/bin/lessc --npm-import=\"prefix=~\" src/index-dev.less ./example/aktivitetsplanfelles/index-dev.css",
    "build:less": "node ./node_modules/less/bin/lessc --npm-import=\"prefix=~\" --compress src/index.less ./build/css/index.css",
    "build:tekster": "node ./build-tekster.js",
    "precommit": "lint-staged",
    "build:js:sbs": "webpack --config webpacksbs.config.js",
    "build:js:fss": "webpack --config webpackfss.config.js",
    "heroku-postbuild": "npm run build:tekster && npm run build:less:dev && npm run build:js -- --env.dev --env.mock"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --single-quote --trailing-comma es5 --tab-width 4",
      "eslint-staged",
      "git add"
    ],
    "*.less": [
      "lesshint-staged",
      "git add"
    ]
  },
