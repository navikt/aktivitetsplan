{
  "name": "aktivitetsplan",
  "version": "2.0.0",
  "description": "Aktivitetsplan",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:gh-pages": "VITE_USE_HASH_ROUTER=true vite",
    "build:dev-ekstern": "vite build --mode=dev-ekstern --base=https://cdn.nav.no/dab/aktivitetsplan-dev-ekstern/build/",
    "build:dev-intern": "vite build --mode=dev-intern --base=https://cdn.nav.no/dab/aktivitetsplan-dev-intern/build/",
    "build:prod-ekstern": "vite build --mode=prod-ekstern --base=https://cdn.nav.no/dab/aktivitetsplan-prod-ekstern/build/",
    "build:prod-intern": "vite build --mode=prod-intern --base=https://cdn.nav.no/dab/aktivitetsplan-prod-intern/build/",
    "build": "cross-env VITE_USE_HASH_ROUTER=true vite build --mode=dev",
    "build:gh-pages": "VITE_USE_HASH_ROUTER=true vite build --mode=dev-ekstern --base=/aktivitetsplan/",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "format-all": "prettier --write 'src/'",
    "lint-staged": "lint-staged",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "preview": "vite preview"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.16.1",
    "@hookform/resolvers": "^3.10.0",
    "@navikt/aksel-icons": "^7.18.0",
    "@navikt/dab-spraksjekk": "^1.0.15",
    "@navikt/ds-css": "^7.21.0",
    "@navikt/ds-react": "^7.21.0",
    "@navikt/ds-tailwind": "^7.21.0",
    "@navikt/ds-tokens": "^7.21.0",
    "@reduxjs/toolkit": "^2.6.1",
    "@sentry/react": "^8.55.0",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-collapse": "^5.1.1",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.56.1",
    "react-modal": "^3.16.3",
    "react-pdf": "^9.2.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.0",
    "rebound": "^0.1.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.1.2",
    "zod": "^3.24.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,less,css,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 10",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.11.24",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/moment-duration-format": "^2.2.6",
    "@types/node": "^22.15.3",
    "@types/react": "^18.3.20",
    "@types/react-collapse": "^5.0.4",
    "@types/react-dom": "^18.3.5",
    "@types/react-modal": "^3.16.3",
    "@types/react-redux": "^7.1.34",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vitest/coverage-v8": "^3.1.2",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vitest": "^0.5.4",
    "husky": "^9.1.7",
    "import-sort-style-module": "^6.0.0",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.5.1",
    "msw": "^2.7.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-import-sort": "0.0.7",
    "tailwindcss": "^3.4.17",
    "vite-plugin-static-copy": "^2.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/navikt/aktivitetsplan.git"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
