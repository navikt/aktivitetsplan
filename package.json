    "build": "npm run build:less && npm run build:js",
    "build:js": "webpack --optimize-minimize",
    "build:less": "node ./node_modules/less/bin/lessc src/index.less ./example/aktivitetsplanfelles/index.css",
    "precommit": "lint-staged"
  },
