{
  "name": "aktivitetsplan",
  "version": "2.0.0",
  "description": "Aktivitetsplan",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:gh-pages": "VITE_USE_HASH_ROUTER=true vite",
    "build:dev-ekstern": "vite build --mode=dev-ekstern",
    "build:dev-intern": "vite build --mode=dev-intern --base=https://cdn.nav.no/dab/aktivitetsplan-dev-intern/build/",
    "build:prod-ekstern": "vite build --mode=prod-ekstern",
    "build:prod-intern": "vite build --mode=prod-intern --base=https://cdn.nav.no/dab/aktivitetsplan-prod-intern/build/",
    "build": "VITE_USE_HASH_ROUTER=true vite build --mode=dev",
    "build:gh-pages": "VITE_USE_HASH_ROUTER=true vite build --mode=dev-ekstern --base=/aktivitetsplan/",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "format-all": "prettier --write 'src/'",
    "lint-staged": "lint-staged",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "preview": "vite preview"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.4.0",
    "@hookform/resolvers": "^3.3.4",
    "@navikt/aksel-icons": "^5.17.3",
    "@navikt/dab-spraksjekk": "^1.0.10",
    "@navikt/ds-css": "^5.17.3",
    "@navikt/ds-react": "^5.17.3",
    "@navikt/ds-tailwind": "^5.17.3",
    "@navikt/ds-tokens": "^5.17.3",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/integrations": "^7.99.0",
    "@sentry/react": "^7.99.0",
    "@sentry/tracing": "^7.99.0",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "classnames": "^2.5.1",
    "date-fns": "^2.30.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-collapse": "^5.1.1",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.0",
    "react-modal": "^3.16.1",
    "react-pdf": "^7.7.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.22.0",
    "rebound": "^0.1.0",
    "redux": "^4.2.1",
    "reselect": "^4.1.8",
    "rollup-plugin-visualizer": "^5.12.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.1.6",
    "vite": "^5.0.12",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-svgr": "^4.2.0",
    "vitest": "^0.34.2",
    "zod": "^3.22.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,less,css,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 10",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@swc/cli": "^0.3.6",
    "@swc/core": "^1.3.107",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/moment-duration-format": "^2.2.6",
    "@types/node": "^18.19.14",
    "@types/react": "^18.2.51",
    "@types/react-collapse": "^5.0.4",
    "@types/react-dom": "^18.2.18",
    "@types/react-modal": "^3.16.3",
    "@types/react-redux": "^7.1.33",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "@vitest/coverage-c8": "^0.29.1",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vitest": "^0.2.8",
    "husky": "^8.0.3",
    "import-sort-style-module": "^6.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "msw": "^2.2.14",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-import-sort": "0.0.7",
    "tailwindcss": "^3.4.1",
    "vite-plugin-static-copy": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/navikt/aktivitetsplan.git"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
